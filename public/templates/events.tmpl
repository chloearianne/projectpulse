{{ define "content" }}
<div class="header">
  <h2>Upcoming Events</h2>
</div><hr />
<div class="row">
  <div class="col-md-8 col-xs-12 main-content">
    <div class="container">
      {{ range $index, $event := .Events }}
        <div class="col-md-4 event">
          <h3>{{ $event.title }}</h3>
          <h4>{{ $event.timestamp }}</h4>
        </div>
      {{ end }}
    </div>
  </div>
</div>
{{ end }}
